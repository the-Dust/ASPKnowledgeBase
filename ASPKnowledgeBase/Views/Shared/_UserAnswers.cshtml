@model IEnumerable<DataAccess.Entities.QuestionLine>

    
@if (Model.Count() != 0)
{
<table class="table table-bordered table-hover">
    <tr>
        <th>№</th>
        <th>Тема</th>
        <th>Автор</th>
        <th>Дата</th>
        <th>Вопрос</th>
        <th>Ответ</th>
    </tr>
    @foreach (var line in Model)
    {
        <tr>
            <td>@line.Id</td>
            <td>@line.Theme.Description</td>
            <td>@line.Author</td>
            <td>@line.Date</td>
            <td>@line.Question</td>
            <td>@line.Answer</td>
        </tr>
    }
</table>
}
else
{
<h4>Поиск не дал результатов</h4>
}

